# Backend Environment Variables
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/reconciliation-db
JWT_SECRET=your-super-secret-jwt-key-here
NODE_ENV=production
PORT=5000

# Frontend URL for CORS (will be your Vercel frontend URL)
FRONTEND_URL=https://your-frontend-app.vercel.app

# Redis URL (if using Redis for Bull queue)
REDIS_URL=redis://localhost:6379

# File upload settings
MAX_FILE_SIZE=10485760
UPLOAD_PATH=./uploads